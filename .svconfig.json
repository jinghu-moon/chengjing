{
  "storage": {
    "backupsDir": "A_backups",
    "compress": false
  },
  "naming": {
    "prefix": "v",
    "dateFormat": "yyyy-MM-dd_HHmm",
    "defaultDesc": "backup"
  },
  "retention": {
    "maxBackups": 50,
    "deleteCount": 10
  },
  "include": [
    "src",
    "public",
    "package.json",
    "package-lock.json",
    "tsconfig.json",
    "tsconfig.node.json",
    "tsconfig.app.json",
    "vite.config.ts",
    "index.html",
    ".env",
    ".env.local",
    "scripts",
    "script"
  ],
  "exclude": [
    "node_modules",
    "dist",
    "build",
    ".git",
    ".DS_Store",
    ".log",
    "src/assets/font"
  ]
}